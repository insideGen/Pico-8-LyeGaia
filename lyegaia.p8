function test_limits(g)
	local t = g.animations[g.tile.animation][g.tile.i]
	if (g.position.x + t.hb.x < 0) g.position.x = 0 - t.hb.x
	if (g.position.x + t.hb.x + t.hb.w > 128) g.position.x = 128 - t.hb.x - t.hb.w
	if (g.position.y + t.hb.y < 0) g.position.y = 0 - t.hb.y
	if (g.position.y + t.hb.y + t.hb.h > 128) g.position.y = 128 - t.hb.y - t.hb.h
end

GItem = {}
function GItem:new()
	local self = setmetatable({}, self)
	self.__index = self
	self.position = { x = 0, y = 0 }
	self.tile = { animation = 'default', i = 1, d = 0 }
	self.animations = {
		default = {
			{ i = 0, w = 1, h = 1, hb = { x = 0, y = 0, w = 16, h = 16 }, f = false, d = 60 }
		}
	}
	self.setPos = function(self, x, y)
		self.position.x = x or self.position.x
		self.position.y = y or self.position.y
	end
	self.addPos = function(self, x, y)
		self.position.x += x
		self.position.y += y
	end
	self.addAnimations = function(self, animations, default)
		for k, v in pairs(animations) do
			self.animations[k] = v
		end
		self.tile.animation = default or self.tile.animation
	end
	self.setAnimation = function(self, animation)
		if self.tile.animation != animation then
			self.tile.animation = animation
			self.tile.i = 1
			self.tile.d = 0
		end
	end
	self.tick = function(self)
		local t = self.animations[self.tile.animation][self.tile.i]
		local c = count(self.animations[self.tile.animation])
		if self.tile.d < t.d then
			self.tile.d += 1
		else
			self.tile.d = 0
			if self.tile.i < c then
				self.tile.i += 1
			else
				self.tile.i = 1
			end
		end
	end
	self.draw = function(self)
		local t = self.animations[self.tile.animation][self.tile.i]
		spr(t.i, self.position.x, self.position.y, t.w, t.h, t.f)
	end
	return self
end

