Player = {}
function Player:new(g)
	local self = setmetatable({}, self)
	self.__index = self
	self.g = g
	self.flip = false
	self.speed = 1
	self.update = function(self, cmd)
		if cmd.left or cmd.right or cmd.up or cmd.down then
			if cmd.left and not cmd.right then
				-- left
				self.flip = true
				self.g:setAnimation('walk_left')
				self.g:addPos(-1, 0)
			elseif not cmd.left and cmd.right then
				-- right
				self.flip = false
				self.g:setAnimation('walk_right')
				self.g:addPos(1, 0)
			end
			if cmd.up and not cmd.down then
				-- up
				if self.flip then self.g:setAnimation('walk_left') else self.g:setAnimation('walk_right') end
				self.g:addPos(0, -1)
			elseif not cmd.up and cmd.down then
				-- down
				if self.flip then self.g:setAnimation('walk_left') else self.g:setAnimation('walk_right') end
				self.g:addPos(0, 1)
			end
		else
			if self.flip then
				self.g:setAnimation('standup_left')
			else
				self.g:setAnimation('standup_right')
			end
		end
	end
	return self
end

function _init()
	headphones = HeadphonesGItem:new()
	headphones:setPos(40, 60)

	cable = CableGItem:new()
	cable:setPos(60, 60)

	cola_can = ColaCanGItem:new()
	cola_can:setPos(80, 60)

	player1 = Player:new(LyeGaiaGItem:new())
	player1.g:setPos(30, 20)

	player2 = Player:new(RaikouGItem:new())
	player2.g:setPos(90, 20)
end

function _update60()
	local cmd_p1 = {
		left = btn(⬅️, 0),
		right = btn(➡️, 0),
		up = btn(⬆️, 0),
		down = btn(⬇️, 0),
		action1 = btn(❎, 0),
		action2 = btn(🅾️, 0)
	}
	local cmd_p2 = {
		left = btn(⬅️, 1),
		right = btn(➡️, 1),
		up = btn(⬆️, 1),
		down = btn(⬇️, 1),
		action1 = btn(❎, 1),
		action2 = btn(🅾️, 1)
	}
	local cmd = {
		left = cmd_p1.left or cmd_p2.left,
		right = cmd_p1.right or cmd_p2.right,
		up = cmd_p1.up or cmd_p2.up,
		down = cmd_p1.down or cmd_p2.down,
		action1 = cmd_p1.action1 or cmd_p2.action1,
		action2 = cmd_p1.action2 or cmd_p2.action2
	}
	if btnp(❎, 0) or btnp(❎, 1) or btnp(🅾️, 0) or btnp(🅾️, 1) then
		local player = player1
		player1 = player2
		player2 = player
	end
	headphones:tick()
	cable:tick()
	cola_can:tick()
	player1:update(cmd_p1)
	player1.g:tick()
	player2:update(cmd_p2)
	player2.g:tick()
	test_limits(player1.g)
	test_limits(player2.g)
end

function _draw()
	cls(7)
	headphones:draw()
	cable:draw()
	cola_can:draw()
	player1.g:draw()
	player2.g:draw()
end

